{
    "name": "qnode-mysql",
    "version": "0.9.23",
    "description": "MySQL client of qnode framework, by Qiang Yiting",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "coverage": "node ./node_modules/istanbul/lib/cli.js cover -color ./node_modules/jasmine/bin/jasmine.js",
        "test": "node ./node_modules/jasmine/bin/jasmine.js",
        "lint": "./node_modules/.bin/eslint -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/",
        "watch": "./node_modules/.bin/esw --watch -f stylish --cache --cache-location ./build/eslint/cache/ --color ./src/"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:qiangyt/qnode-mysql.git"
    },
    "author": "Qiang Yiting <qiangyt@wxcount.com> (https://github.com/qiangyt)",
    "keywords": [
        "mysql",
        "node",
        "node.js"
    ],
    "bugs": {
        "url": "https://github.com/qiangyt/qnode-mysql/issues"
    },
    "engines": {
        "node": ">=8"
    },
    "private": false,
    "license": "MIT",
    "dependencies": {
        "bluebird": "3.5.1",
        "lodash": "4.17.4",
        "mysql": "2.13.0",
        "qnode-beans": "1.2.6",
        "qnode-config": "1.0.8",
        "qnode-error": "1.0.6",
        "qnode-log": "1.0.4",
        "sequelize": "3.30.4"
    },
    "devDependencies": {
        "@types/lodash": "4.14.70",
        "@types/mysql": "0.0.34",
        "@types/sequelize": "4.0.66",
        "@types/sinon": "2.3.3",
        "babel": "6.23.0",
        "babel-cli": "6.24.1",
        "babel-eslint": "7.2.3",
        "babel-preset-es2015": "6.24.1",
        "babel-preset-stage-0": "6.24.1",
        "eslint": "4.3.0",
        "eslint-stylish": "0.2.0",
        "eslint-watch": "3.1.2",
        "gulp": "3.9.1",
        "gulp-sourcemaps": "2.5.1",
        "gulp-typescript": "3.2.1",
        "gulp-util": "3.0.8",
        "istanbul": "1.1.0-alpha.1",
        "jasmine": "2.7.0",
        "merge2": "1.0.3",
        "rimraf": "2.6.1",
        "sinon": "2.4.1",
        "typescript": "2.4.2"
    },
    "browser": {
        "os": false
